<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="auto3d">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference" />
        <attr name="mvAnimDuration" format="integer|reference" />
        <attr name="mvTextSize" format="dimension|reference" />
        <attr name="mvTextColor" format="color|reference" />
        <attr name="mvSingleLine" format="boolean" />
        <attr name="mvGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="mvDirection">
            <enum name="bottom_to_top" value="0" />
            <enum name="top_to_bottom" value="1" />
            <enum name="right_to_left" value="2" />
            <enum name="left_to_right" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NoPaddingTextView">
        <attr name="removeDefaultPadding" format="boolean" />
    </declare-styleable>


    <declare-styleable name="LabelLayout">
        <attr name="labelPaddingTop" format="dimension" />
        <attr name="labelPaddingBottom" format="dimension" />
        <attr name="labelPaddingLeft" format="dimension" />
        <attr name="labelPaddingRight" format="dimension" />
        <attr name="labelMarginLeft" format="dimension" />
        <attr name="labelMarginRight" format="dimension" />
        <attr name="labelTextSize" format="dimension" />
        <!--是否充满一行-->
        <attr name="labelFullLine" format="boolean" />
    </declare-styleable>


    <declare-styleable name="SideBar">
        <attr name="sideBarTextColor" format="color" />
        <attr name="sideBarTextSize" format="dimension" />
    </declare-styleable>


    <declare-styleable name="StepSlideView">
        <attr name="ssvTextSize" format="dimension" />
        <attr name="ssvNormalColor" format="color" />
        <attr name="ssvSelectTextColor" format="color" />
        <attr name="ssvNormalPointRadius" format="dimension" />
        <attr name="ssvSelectPointRadius" format="dimension" />
        <attr name="ssvSpaceInTextPoint" format="dimension" />
    </declare-styleable>


    <declare-styleable name="TrainMonthView">
        <attr name="tmvDayTextSize" format="dimension" />
        <attr name="tmvTopTextSize" format="dimension" />
        <attr name="tmvDownTextSize" format="dimension" />
        <attr name="tmvDayHeight" format="dimension" />
        <attr name="tmvVerticalDaySpace" format="dimension" />
    </declare-styleable>


    <declare-styleable name="ShadowLayout">
        <attr name="sl_cornerRadius" format="dimension" />
        <attr name="sl_shadowRadius" format="dimension" />
        <attr name="sl_shadowColor" format="color" />
        <attr name="sl_dx" format="dimension" />
        <attr name="sl_dy" format="dimension" />
        <attr name="sl_borderColor" format="color" />
        <attr name="sl_borderWidth" format="dimension" />
        <attr name="sl_shadowSides">
            <flag name="TOP" value="0x001" />
            <flag name="RIGHT" value="0x002" />
            <flag name="BOTTOM" value="0x004" />
            <flag name="LEFT" value="0x008" />
            <flag name="ALL" value="0x00f" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SwitchView">
        <attr name="switch_bg_close_color" format="color" />
        <attr name="switch_bg_open_color" format="color" />
        <attr name="switch_strock_color" format="color" />
        <attr name="switch_ball_color" format="color" />
        <attr name="switch_ball_shadow_color" format="color" />
        <attr name="switch_bg_strock_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="NineImageLayout">
        <attr name="sapcing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="WaveProgressView">
        <attr name="wave_progress_rotate" format="boolean"/>
        <attr name="wave_progress_color" format="color"/>
    </declare-styleable>
</resources>